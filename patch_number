+FILE(WRITE "/home/ubuntu20/Bureau/TD6/GIT__SVN__diff_patch/statelib_new/build3/${CMAKE_INSTALL_MANIFEST}" "")
+FOREACH(file ${CMAKE_INSTALL_MANIFEST_FILES})
+  FILE(APPEND "/home/ubuntu20/Bureau/TD6/GIT__SVN__diff_patch/statelib_new/build3/${CMAKE_INSTALL_MANIFEST}" "${file}\n")
+ENDFOREACH(file)
